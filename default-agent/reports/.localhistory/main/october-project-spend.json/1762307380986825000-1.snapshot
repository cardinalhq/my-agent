{
  "name": "october-project-spend",
  "text": "total spend in October 2025 broken down by service",
  "query_type": "bigquery_sql",
  "query": "SELECT \n  service.description AS service_name,\n  SUM(cost) AS total_spend\nFROM `chip_billing.gcp_billing_export_resource_v1_01D01E_9B88C7_8078BC`\nWHERE DATE(usage_start_time) BETWEEN '2025-10-01' AND '2025-10-31'\nGROUP BY service.description\nORDER BY total_spend DESC",
  "status": "private",
  "meta": {
    "sql_flow_diagram": "flowchart TD\n    Q[\"Question: Total spend in October 2025 broken down by service\"]\n    T1[(\"gcp_billing_export_resource_v1_01D01E_9B88C7_8078BC\")]\n    F1{\"Filter: DATE(usage_start_time) BETWEEN '2025-10-01' AND '2025-10-31'\"}\n    G1{\"GROUP BY service.description\"}\n    AGG{\"SUM(cost) AS total_spend\"}\n    S1{\"ORDER BY total_spend DESC\"}\n    R[\"Result: Service breakdown with total spend for October 2025\"]\n\n    Q --\u003e T1\n    T1 --\u003e F1\n    F1 --\u003e G1\n    G1 --\u003e AGG\n    AGG --\u003e S1\n    S1 --\u003e R"
  },
